[Unit]
Description=Gitlab target.
# gitlab-@WEBSERVER@ is already required by gitlab-workhorse
Requires=gitlab-workhorse.service gitlab-sidekiq.service
@OPTIONAL_REQUIRES@
@OPTIONAL_AFTER@
Wants=gitlab-gitaly.service
# Add Wants symlinks to postgresql-12.service should to be added to
#   /etc/systemd/systemd/gitlab.target.wants
@OPTIONAL_WANTS@

[Install]
WantedBy=multi-user.target
Alias=gitlab.target
